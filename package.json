{
  "name": "sadelica-nw",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "resolutions": {
    "node-fetch": "2.6.7"
  },
  "scripts": {
    "start": "yarn watch",
    "start:dist": "http-server ./dist -o index.html -g -p 8081",
    "start:only": "yarn vite",
    "once-keys": "node scripts/once-keys",
    "ffmpeg": "cp node_modules/ffmpeg-static/ffmpeg* node_modules/.bin",
    "watch": "tsc --noEmit && yarn start:only",
    "test": "jest --coverage test",
    "test:watch": "jest --watch",
    "clean": "rm -rf .build && rm -rf dist && rm -rf *.map main.js",
    "tsc": "tsc --project tsconfig.prod.json",
    "vite": "concurrently -n 1,2 \"vite serve src --config ./vite.config.js --host\" \"http-server -c-1\"",
    "vite:build": "vite build src --config ./vite.config.js && node scripts/build-vite.js src"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.2",
    "@testing-library/preact": "^2.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^11.9.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "audiosprite": "^0.7.2",
    "bestzip": "^2.2.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.7.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "ffmpeg-static": "^4.4.1",
    "html-minifier": "^4.0.0",
    "http-server": "^14.1.0",
    "jest": "^27.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jszip": "3.7.0",
    "prettier": "^2.2.1",
    "terser": "^4.6.11",
    "tiled-types": "^1.3.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.4",
    "uglifycss": "0.0.29",
    "uuid": "^8.3.2",
    "vite": "^2.7.12",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "picostyle": "^2.2.0",
    "preact": "10.5.7",
    "preact-custom-scrollbars": "^4.0.4"
  }
}
