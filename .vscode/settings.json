{
  "cSpell.words": ["Dropzone", "interactables", "NOLINTNEXTLINE", "NOLINT"],
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact",
    "*.graphqls": "graphql",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "any": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "chrono": "cpp",
    "codecvt": "cpp",
    "compare": "cpp",
    "complex": "cpp",
    "concepts": "cpp",
    "condition_variable": "cpp",
    "cstdint": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "fstream": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "ranges": "cpp",
    "shared_mutex": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "stop_token": "cpp",
    "streambuf": "cpp",
    "thread": "cpp",
    "cinttypes": "cpp",
    "typeinfo": "cpp",
    "valarray": "cpp",
    "*.def": "cpp",
    "*.inc": "cpp",
    "future": "cpp",
    "numbers": "cpp",
    "semaphore": "cpp",
    "span": "cpp",
    "typeindex": "cpp",
    "variant": "cpp"
  },
  "clang-tidy.blacklist": ["entt.*"],
  "clang-tidy.compilerArgsBefore": ["-xc++"],
  "clang-tidy.compilerArgs": ["-std=c++17", "-stdlib=libstdc++", "-Icpp"],
  "clang-tidy.checks": [
    "misc-*",
    "bugprone-*",
    "cppcoreguidelines-*",
    "portability-*",
    "-clang-analyzer-*",
    "-misc-unused-parameters",
    "-misc-non-private-member-variables-in-classes",
    "-cppcoreguidelines-non-private-member-variables-in-classes",
    "-cppcoreguidelines-avoid-non-const-global-variables",
    "-cppcoreguidelines-init-variables",
    "-cppcoreguidelines-avoid-magic-numbers",
    "-cppcoreguidelines-pro-type-vararg",
    "-cppcoreguidelines-prefer-member-initializer",
    "-bugprone-easily-swappable-parameters"
  ],
  "cpplint.filters": [
    "-whitespace",
    "-legal",
    "-readability/braces",
    "-build/include_order",
    "-build/header_guard",
    "-build/namespaces",
    "-runtime/string",
    "-build/include"
  ],
  "C_Cpp.default.configurationProvider": "",
  "C_Cpp.codeAnalysis.clangTidy.checks.enabled": [
    "misc-*",
    "bugprone-*",
    "cppcoreguidelines-*",
    "portability-*"
  ],
  "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
    "clang-analyzer-*",
    "misc-unused-parameters",
    "misc-non-private-member-variables-in-classes",
    "cppcoreguidelines-non-private-member-variables-in-classes",
    "cppcoreguidelines-avoid-non-const-global-variables",
    "cppcoreguidelines-init-variables",
    "cppcoreguidelines-avoid-magic-numbers",
    "cppcoreguidelines-pro-type-vararg",
    "cppcoreguidelines-prefer-member-initializer",
    "bugprone-easily-swappable-parameters"
  ],
  "C_Cpp.codeAnalysis.clangTidy.codeAction.showDisable": true,
  "C_Cpp.codeAnalysis.exclude": {
    "SDL_*": true
  },
  "C_Cpp.codeAnalysis.maxConcurrentThreads": null,
  "errorLens.exclude": ["Error while processing"]
}
