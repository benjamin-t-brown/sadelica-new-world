{
  "cSpell.words": [
    "Dropzone",
    "interactables"
  ],
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.ts": "typescript",
    "*.tsx": "typescriptreact",
    "*.graphqls": "graphql",
    "string": "cpp",
    "sstream": "cpp",
    "memory": "cpp",
    "functional": "cpp",
    "iostream": "cpp",
    "algorithm": "cpp",
    "vector": "cpp",
    "stack": "cpp"
  },
  "cmake.configureOnOpen": true,
  "cmake.sourceDirectory": "${workspaceFolder}/.",
  "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
    "clang-diagnostic-error",
    "clang-diagnostic-error",
    "cppcoreguidelines-pro-type-vararg",
    "bugprone-easily-swappable-parameters",
    "clang-diagnostic-error"
  ]
}