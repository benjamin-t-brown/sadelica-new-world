{
  "name": "in2",
  "version": "1.2.0",
  "description": "IN2 Tree Builder",
  "main": "src-web/index.js",
  "scripts": {
    "start": "yarn watch",
    "server": "node src-srv",
    "webpack-server": "webpack serve",
    "watch": "concurrently -n srv,webpack \"yarn server\" \"yarn webpack-server\"",
    "build:css": "node build.js build-css",
    "build:compiled": "node src-compile/compiler.js",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --require @babel/register --timeout 99999",
    "babelify": "node_modules/.bin/babel src-compile/main.compiled.js --out-file src-compile/main.compiled.js --presets @babel/preset-env"
  },
  "keywords": [
    "game",
    "tree",
    "dialog"
  ],
  "author": "Benjamin Brown",
  "license": "ISC",
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      [
        "module-resolver",
        {
          "root": [
            "./src-web"
          ]
        }
      ]
    ],
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": "last 1 chrome versions"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "gzip-js": "^0.3.2",
    "http-server": "^0.12.3",
    "jquery": "^3.5.0",
    "jsplumb": "^2.15.6",
    "minimist": "^1.2.5",
    "open-cli": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dom-factories": "^1.0.2",
    "react-dropzone": "^11.5.3",
    "react-jss": "^10.3.0",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/node": "^7.20.2",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.18.9",
    "@jsplumb/browser-ui": "^5.3.9",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "elliptic": ">=6.5.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.10.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "mocha": "^6.1.4",
    "prettier": "^2.7.1",
    "terser": "^4.8.0",
    "uglifycss": "0.0.29",
    "webpack": "^5.6.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.0"
  }
}
